{
  "manifest_version": 2,

  "name": "Squirt for Chrome",
  "description": "Speed read the web, one word at a time.",
  "version": "0.1",
  "icons": { "128": "logo.png" },


  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "logo.png"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  }
}